{
  "name": "spaceman",
  "version": "0.0.3",
  "description": "react based workspace - layout - manager",
  "main": "lib/index.js",
  "scripts": {
    "dev": "babel src --stage 0 --out-dir lib --watch",
    "prebuild": "rm -rf lib && rm -rf browser && mkdir browser",
    "build:lib": "babel src --stage 0 --out-dir lib",
    "build:browser": "browserify lib/Spaceman.js --standalone spaceman --debug | exorcist browser/spaceman.js.map > browser/spaceman.js",
    "build": "npm run build:lib && npm run build:browser",
    "_prepublish": "npm run build"
  },
  "keywords": [
    "layout",
    "manager",
    "workspace",
    "react",
    "window"
  ],
  "author": "Polgár András",
  "license": "MIT",
  "dependencies": {
    "es6-symbol": "^2.0.1",
    "eventman": "git://github.com/azazdeaz/eventman",
    "lodash": "^3.6.0"
  },
  "peerDependencies": {
    "react": ">=0.13 <0.14.0"
  },
  "devDependencies": {
    "babel": "^5.0.10",
    "babel-eslint": "^3.1.5",
    "babelify": "^6.0.2",
    "browser-sync": "^2.6.1",
    "browserify": "^9.0.8",
    "eslint": "^0.21.2",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.1.0",
    "gulp-callback": "0.0.3",
    "gulp-filesize": "0.0.6",
    "gulp-plumber": "^1.0.0",
    "gulp-rimraf": "^0.1.1",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.0"
  }
}
