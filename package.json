{
  "name": "spaceman",
  "version": "0.0.0",
  "description": "react workspace - layout - manager",
  "main": "lib/Spaceman.js",
  "scripts": {
    "dev": "babel src --stage 0 --out-dir lib --watch",
    "prebuild": "rm -rf lib && rm -rf browser && mkdir browser",
    "build:lib": "babel src --stage 0 --out-dir lib",
    "build:browser": "browserify lib/Spaceman.js --standalone spaceman --debug | exorcist browser/spaceman.js.map > browser/spaceman.js",
    "build": "npm run build:lib && npm run build:browser",
    "prepublish": "npm run build"
  },
  "keywords": [
    "layout",
    "manager",
    "workspace",
    "react"
  ],
  "author": "Polgár András",
  "license": "MIT",
  "dependencies": {
    "lodash.has": "^3.0.0",
    "lodash.isarray": "^3.0.1",
    "lodash.merge": "^3.0.3",
    "lodash.pull": "^3.0.1",
    "lodash.isnumber": "^3.0.1",
    "eventman": "git://github.com/azazdeaz/eventman"
  },
  "devDependencies": {
    "babel": "^5.0.10"
  }
}
