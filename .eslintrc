{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "comma-dangle": [0],
    "curly": [0],
    "strict": [0],
    "semi": [2, "never"],
    "no-use-before-define": [1, "nofunc"],
  }
}
